<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/item_lock"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/item_lock"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="90px"
            android:orientation="horizontal"
            android:background="@color/white"
            >
            <TextView
                android:id="@+id/in_child_num"
                android:layout_width="120px"
                android:layout_height="match_parent"
                android:text="项目编号"
                android:paddingTop="10px"
                android:paddingBottom="10px"
                android:gravity="center"
                android:textColor="@color/text_fz"
                android:textSize="@dimen/text_samll"
                />
            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/danhui"
                />
            <TextView
                android:id="@+id/in_child_name"
                android:layout_width="200px"
                android:layout_height="match_parent"
                android:text="项目名称"
                android:paddingTop="10px"
                android:paddingBottom="10px"
                android:textColor="@color/text_fz"
                android:gravity="center"
                android:textSize="@dimen/text_samll"
                />
            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/danhui"
                />
            <TextView
                android:id="@+id/in_child_result"
                android:layout_width="150px"
                android:layout_height="match_parent"
                android:text="检验结果"
                android:gravity="center"
                android:paddingTop="10px"
                android:paddingBottom="10px"
                android:textColor="@color/text_fz"
                android:textSize="@dimen/text_samll"
                />
            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/danhui"
                />
            <TextView
                android:id="@+id/in_child_dw"
                android:layout_width="120px"
                android:paddingTop="10px"
                android:paddingBottom="10px"
                android:layout_height="match_parent"
                android:text="单位"
                android:gravity="center"
                android:textColor="@color/text_fz"
                android:textSize="@dimen/text_samll"
                />
            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/danhui"
                />
            <TextView
                android:id="@+id/in_child_ts"
                android:layout_width="130px"
                android:paddingTop="10px"
                android:paddingBottom="10px"
                android:layout_height="match_parent"
                android:text="检验提示"
                android:textColor="@color/text_fz"
                android:gravity="center"
                android:textSize="@dimen/text_samll"
                />
            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/danhui"
                />
            <TextView
                android:id="@+id/in_child_zcz"
                android:layout_width="150px"
                android:layout_height="match_parent"
                android:text="正常值"
                android:paddingTop="10px"
                android:paddingBottom="10px"
                android:gravity="center"
                android:textColor="@color/text_fz"
                android:textSize="@dimen/text_samll"
                />
            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="@color/danhui"
                />
            <TextView
                android:id="@+id/in_child_dh"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="项目代号"
                android:paddingTop="10px"
                android:paddingBottom="10px"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/text_fz"
                android:textSize="@dimen/text_samll"
                />
        </LinearLayout>

        <View
            android:id="@+id/in_child_line"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/danhui"
            />
    </LinearLayout>

</LinearLayout>